input x : Int

output a := x.offset(by: -1).defaults(to: 10) +  1
output b := x.offset(by: -2).defaults(to: 100) + 1
output c := a + b + x
output d := a + c

output e @2kHz := e.offset(by: -1).defaults(to: 0) + 1
output f @1kHz := x.aggregate(over: 0.003s, using: sum) 
output g := e + f + d.hold(or: 100)
