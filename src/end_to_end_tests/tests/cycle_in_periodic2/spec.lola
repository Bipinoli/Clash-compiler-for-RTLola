input x: Int

output a @1kHz := a.offset(by: -1).defaults(to: 0) + 1
output b @1kHz := a.hold(or: 0) + f.offset(by: -1).defaults(to: 0) + 1 + f.offset(by: -2).defaults(to: 0) 
output c @1kHz := b.offset(by: -1).defaults(to: 0) + d.hold(or: 0)
output d @1kHz := b.offset(by: -1).defaults(to: 0) + e.offset(by: -1).defaults(to: 1)
output e @2kHz := c.hold(or: 0) + 1
output f @1kHz := d.hold(or: 2) + 1
